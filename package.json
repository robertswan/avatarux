{
  "deploy": "./deploy",
  "license": "UNLICENSED",
  "scripts": {
    "start": "static -a 0.0.0.0 deploy",
    "clean": "cross-var \"rm -rf $npm_package_deploy/src && node scripts/file/mkdir.js $npm_package_deploy/src\"",
    "game-debug": "cross-var \"node scripts/build-game-src.js -t game -b debug\"",
    "game-debug-watch": "cross-var \"node scripts/build-game-src.js -t game -b debug -w\"",
    "test": "jest --coverage=true",
    "lint": "cross-var eslint src/**/*.js game-index.js",
    "lint-fix": "cross-var eslint src/**/*.js game-index.js --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^23.1.1",
    "pixi.js": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "babel-eslint": "10.1.0",
    "babelify": "10.0.0",
    "browserify": "14.5.0",
    "cross-var": "1.1.0",
    "envify": "4.1.0",
    "eslint": "6.8.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "node-static": "0.7.10",
    "watchify": "3.11.0"
  }
}
